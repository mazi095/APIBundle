{
    "type": "Bundle",
    "name": "Mazi/AdrRestApi",
    "discription": "Action-Domain-Responder API with swagger documentation for symfony framework",
    "authors": [
        {
            "name": "Sergey Shev",
            "email": "Mazi095@gmail.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "jms/serializer-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "zircote/swagger-php": "3.x-dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mazi\\AdrRestApi\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
